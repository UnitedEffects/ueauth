{
  "name": "UE-Auth",
  "version": "0.13.4",
  "description": "UE Auth is a multi-tenant OIDC Provider intended to serve small to midsize organization that require functionality similar to those provided by enterprise Identity and Access SaaS providers but without the costs associated to the full scale of those solutions.",
  "private": true,
  "scripts": {
    "start": "node ./src/start.js",
    "dev": "NODE_ENV=dev nodemon ./src/start.js",
    "dist": "nodemon ./dist/start.js",
    "build": "yarn && yarn test && babel src -d dist",
    "package": "yarn && yarn test && babel src -d dist && sls package",
    "deploy": "yarn && yarn test && babel src -d dist && sls deploy",
    "simple-deploy": "sls deploy",
    "test": "jest",
    "test-notifications": "nodemon ./test/testserver.js"
  },
  "keywords": [
    "docker",
    "lambda",
    "service",
    "microservice",
    "auth",
    "oidc",
    "sso",
    "mfa",
    "permissions",
    "identity",
    "access",
    "authentication",
    "authorization"
  ],
  "author": "United Effects LLC",
  "person": {
    "name": "Bo Motlagh",
    "email": "bmotlagh@unitedeffects.com",
    "url": "https://unitedeffects.com"
  },
  "homepage": "https://github.com/UnitedEffects/ueauth",
  "bugs": {
    "url": "https://github.com/UnitedEffects/ueauth/issues",
    "email": "team@unitedeffects.com"
  },
  "logo": "https://ue-platform-content.s3.amazonaws.com/6e5eab3d-d4e6-4a18-87c5-18be5e46f507",
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.7.4",
    "@hapi/boom": "^9.1.0",
    "axios": "^0.21.1",
    "basic-auth": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.13.2",
    "cookie-parser": "^1.3.5",
    "crypto-random-string": "^3.2.0",
    "express": "^4.17.1",
    "express-openapi-validate": "^0.6.1",
