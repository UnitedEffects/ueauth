{
  "name": "boilerpate",
  "version": "1.0.0",
  "description": "A microservice boilerplate that runs optionally as docker or lambda",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start": "node ./src/start.js",
    "dev": "NODE_ENV=dev nodemon ./src/start.js",
    "dist": "nodemon ./dist/start.js",
    "build": "yarn && babel src -d dist",
    "package": "yarn && babel src -d dist && sls package",
    "deploy": "yarn && babel src -d dist && sls deploy",
    "test": "jest",
    "lintFix": "eslint . --fix"
  },
  "keywords": [
    "docker",
    "lambda",
    "service",
    "microservice",
    "boilerplate"
  ],
  "author": "theBoEffect",
  "url": "https://theboeffect.com",
  "logo": "https://cdn-images-1.medium.com/max/280/1*vuHoiLlmWjuTJ9zK98jFtQ@2x.png",
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.7.4",
    "@hapi/boom": "^9.1.0",
    "axios": "^0.19.2",
    "basic-auth": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.13.2",
    "cookie-parser": "^1.3.5",
    "express": "^4.17.1",
    "express-openapi-validate": "^0.5.1",
    "json-schema-ref-parser": "^7.1.2",
    "json-schema-resolve-allof": "^1.5.0",
    "jsonpatch": "^3.0.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.4",
    "morgan": "^1.9.1",
    "odata-v4-mongodb": "^0.1.12",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pug": "^2.0.4",
    "regenerator-runtime": "^0.13.3",
    "serverless-http": "^2.3.2",
    "uuidv4": "^6.0.6",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.1.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-domain-manager": "^2.6.6",
    "supertest": "^4.0.2"
  },
  "resolutions": {
    "lodash": "^4.17.15"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  },
  "jest": {
    "verbose": true
  }
}
