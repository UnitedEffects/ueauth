doctype html
html
    head
        meta(charset='utf-8')
        meta(name='viewport' content='width=device-width, initial-scale=1')
        meta(http-equiv='x-ua-compatible' content='ie=edge')
        title= title
        link(rel="icon" type="image/png" href=assets + "favicon.png" sizes="100x100")
        link(rel='stylesheet', href='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css', type='text/css')
        link(rel='stylesheet', href='https://www.unpkg.com/bootstrap/dist/css/bootstrap-grid.min.css', type='text/css')
        link(rel='stylesheet', href='https://www.unpkg.com/bootstrap/dist/css/bootstrap-reboot.min.css', type='text/css')
        link(rel='stylesheet', href='https://use.fontawesome.com/releases/v5.6.3/css/all.css', type='text/css')
        link(rel='stylesheet' type='text/css' href=assets + 'template/fonts/icon-font.min.css')
        link(rel='stylesheet' type='text/css' href=assets + 'template/animate/animate.css')
        link(rel='stylesheet' type='text/css' href=assets + 'template/css-hamburgers/hamburgers.min.css')
        link(rel='stylesheet' type='text/css' href=assets + 'template/animsition/css/animsition.min.css')
        link(rel='stylesheet' type='text/css' href=assets + 'template/select2/select2.min.css')
        link(rel='stylesheet' type='text/css' href=assets + 'template/daterangepicker/daterangepicker.css')
        link(rel='stylesheet' type='text/css' href=assets + 'template/util.css')
        link(rel='stylesheet' type='text/css' href=assets + 'template/main.css')
        if customFonts
            link(rel='stylesheet' type='text/css' href=customFonts)
        link(rel='stylesheet', href=assets + 'css/views.css', type='text/css')
        //script(nonce=nonce, src='https://www.unpkg.com/jquery@3.4.1/dist/jquery.min.js', type='text/javascript')
        script(nonce=nonce, src="https://code.jquery.com/jquery-3.6.0.slim.min.js", crossorigin="anonymous", type='text/javascript')
        script(nonce=nonce, src='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js', type='text/javascript')
        script(nonce=nonce, src=assets + 'template/animsition/js/animsition.min.js')
        script(nonce=nonce, src=assets + 'template/select2/select2.min.js')
        script(nonce=nonce, src=assets + 'template/daterangepicker/moment.min.js')
        script(nonce=nonce, src=assets + 'template/daterangepicker/daterangepicker.js')
        script(nonce=nonce, src=assets + 'js/main.js')
        script(nonce=nonce, src=assets + 'js/popper/popper.min.js')
        if (bgGradientLow && bgGradientHigh)
            style.
                body {
                    width: 100%;
                    height: 100%;
                    background: linear-gradient(279.3deg, #{bgGradientLow} 4.36%, #{bgGradientHigh} 96.71%);
                }
        else
            style.
                body {
                    width: 100%;
                    height: 100%;
                    background: linear-gradient(279.3deg, #131525 4.36%, #1D2035 96.71%);
                }
        block content